
void setup(){
// sets the canvas size to 750pi by 750pi  
  size(750,750);
// sets back ground color to a shade of black  
  background(255);
// sets frame rate to 60 frames per second  
  frameRate(60);
}
  
void draw(){
  fill(20,90,30);
  noStroke();
  rect(250,250,100,100);
  fill(20,100,70);
  noStroke();
  rect(350,250,100,100);
  fill(20,100,70);
  noStroke();
  rect(400,400,100,100);
  fill(20,90,30);
  noStroke();
  rect(200,400,100,100);
  
  
  
}


