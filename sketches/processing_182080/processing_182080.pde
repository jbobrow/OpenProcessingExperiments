
size (400,400);
background (72,61,139);
fill(220,  20, 60,10);
for (int i=0; i<400; i=i+10) 
{ 
rect(0,0,i,i);
}
//line
if (0>1){
stroke(255,255,255,100);
line(0,400,200,0);
line(0,400,400,200);
line(0,400,400,0);
}
else
{
  stroke(255,  228,  181,200);
  line(400,400,0,0);
  line(400,400,250,0);
  line(400,400,0,250);
  line(400,400,350,0);
  line(400,400,0,350);
}

//circles
noStroke();
fill(72,61,139,70);
ellipse(200,200,200,200);
ellipse(200,200,100,100);
//bird
stroke(220,  20, 60);
fill(220,  20, 60,70);
//tail
triangle (400,400,300,300,250,300);
triangle (250,300,330,330,300,300);
triangle (250,300,310,360,300,300);
//body
triangle (250,300,300,300,260,230);
triangle (260,230,250,300,150,220);
//wing
triangle(200,266,235,200,300,230);
triangle(235,200,300,230,350,150);
triangle(200,266,260,230,350,250);
//neck
triangle (260,230,150,220,180,180);
//head
triangle(180,180,150,220,160,180);
triangle(128,200,150,220,160,180);
//beak
triangle(128,200,100,210,150,220);

