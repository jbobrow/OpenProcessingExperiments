  for(int i=0; i<31; i++){
    for(int j=0; j<31; j++){
      grays[i][j] = color(255);
    }
  }

    
}
void draw(){
  
  for(int i=0; i<31; i++){
    for(int j=0; j<31; j++){
      if(mouseX >= xpos[j] && mouseX <= xpos[j] + width/31 && mouseY >= ypos[i] && mouseY <= ypos[i]+height/31){
        fill(178.5);      
//        if(mousePressed){
//          grays[i][j] = colors[i][j];
//        }
      } else {
        fill(grays[i][j]);
      }

      rect(xpos[j], ypos[i], width/31, height/31);
    }
  }
}

void mouseDragged(){
   for(int i=0; i<31; i++){
    for(int j=0; j<31; j++){
      if(mouseX >= xpos[j] && mouseX <= xpos[j] + width/31 && mouseY >= ypos[i] && mouseY <= ypos[i]+height/31){
        fill(colors[i][j]);
        grays[i][j] = colors[i][j];

      } else {
        fill(grays[i][j]);
      }      
      rect(xpos[j], ypos[i], width/31, height/31);
    }
  }
}

void keyPressed(){
  if(key=='a'){
    for(int i=0; i<31; i++){
      for(int j=0; j<31; j++){
        grays[i][j] = color(255);
      }
    }    
  }else if(key=='b'){
    for(int i=0; i<31; i++){
      for(int j=0; j<31; j++){
        if(grays[i][j] != color(255)){
          grays[i][j] = color(0);
        }
      }
    }
  }
}

