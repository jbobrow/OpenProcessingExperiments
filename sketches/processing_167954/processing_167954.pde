
int x=0;
void setup()
{
background(#993333);
size(1000,500);
}
void draw()
{
background(#993333);
stroke(#993333);
ellipse(250,250,250,250);
fill(0);
ellipse(250,250,400,400);
fill(#33CC66);
stroke (0);
rect(x+177,150,144,200);
fill(#993333);
rect(x+155,150,30,110);
rect(x+314,150,30,110);
ellipse(x+250,150,200,80);
fill(#993333);
ellipse(x+250,350,150,100);
fill(0);
ellipse(x+220,220,50,50);
ellipse(x+280,220,50,50);
fill(#993333);
ellipse(x+220,220,20,20);
ellipse(x+280,220,20,20);
fill(0);
ellipse(x+250,350,100,50);
fill(#ffffff);
ellipse(x+250,150,100,50);
x=x+1;
}




