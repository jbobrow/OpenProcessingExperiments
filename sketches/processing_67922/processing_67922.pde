
void setup(){
size(400,400);}
void draw(){
background(255);
smooth();
noFill();
stroke(0,0,255);
strokeWeight(5);
ellipse(100,100,100,100);
noFill();
stroke(255,255,0);
ellipse(150,150,100,100);
