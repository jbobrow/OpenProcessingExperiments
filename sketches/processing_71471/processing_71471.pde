void setup()
{
  size(300,300);
}
void draw()
{
}
void mouseDragged()
{
 fill(59,9,100);
 ellipse(mouseX,mouseY,60,60); 
 fill(140,26,185);
 ellipse(mouseX,mouseY,50,50); 
 fill(217,124,255);
 ellipse(mouseX,mouseY,30,30); 
}
void mousePressed()
{
  if(mouseButton==RIGHT);
  textSize(50);
  fill(255,248,18);
 text("  Stephanie",mouseX,mouseY);
}
 void keyPressed()
{
  background(255);
}
