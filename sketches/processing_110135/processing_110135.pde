

size(500,500);
smooth();
strokeJoin(ROUND);

//face//
ellipse(200,200,300,300);

//nose//
line(200,200,170,240);
line(170,240,190,260);

//left eyes//
line(100,160,180,160);
noFill();
arc(140,160,80,50,0,PI);

//right eyes//
line(200,160,300,160);
noFill();
arc(250,160,100,100,0,PI);

//mouth//
strokeWeight(20);
ellipse(240,300,80,80);

//mole//
strokeWeight(17);
point(120,270);

//left eye point//
strokeWeight(14);
point(140,175);

//right eye point//
strokeWeight(25);
point(250,180);






