
void setup()
{
background(255);
smooth();
noFill();
strokeWeight(10);
size(530,400);
}

void draw()
{
 background(255);

//Azul
stroke(10,10,200);
ellipse(150,200,100,100);

//Negro
stroke(10);
ellipse(270,200,100,100);

//Rojo
stroke(255,0,0);
ellipse(390,200,100,100);

//Amarillo
stroke(255,255,0);
ellipse(210,250,100,100);

//Verde
stroke(0,255,90);
ellipse(330,250,100,100);

}

