////////////////MIRILLES
void mirillahora(){
  fill(0);
  text(hour(),75,336);
  textSize(midatext);
  
  if(grow==true){
    midapilota = midapilota + interval;
  } 
  else { 
    midapilota = midapilota - interval;
  }
  
  if(midapilota > midamaxim){
    grow = false;
  }
  if(midapilota < midaminima){
    grow = true;
  }
  noFill();
  stroke(0,0,0,100);
  strokeWeight(2);
  ellipse(95,325,midapilota,midapilota);
  fill(0,0,0,50);
  line(95,290,95,360);
  line(60,325,130,325);
}
  
  void mirillaminuts(){
  fill(0);
  text(minute(),283,336);
  textSize(30);
   if(grow==true){
