
size(1000, 800);
int y = height/2;
int d = width/2;

smooth();
background(#ffffff);


// Thick arc on perimeter
stroke(#3B3B39);
strokeWeight(12);
strokeCap(SQUARE);
arc(d, y, 600, 600, PI*-0.5, PI*1.0);

stroke(#3B3B39);
strokeWeight(12);
strokeCap(SQUARE);
arc(d, y, 570, 570, PI*-0.5, PI*1.0);

stroke(#3B3B39);
strokeWeight(12);
strokeCap(SQUARE);
arc(d, y, 270, 270, PI*-0.2, PI*1.0);

stroke(#3B3B39);
strokeWeight(12);
strokeCap(SQUARE);
arc(d, y, 240, 240, PI*-0.4, PI*1.0);

stroke(#3B3B39);
strokeWeight(12);
strokeCap(SQUARE);
arc(d, y, 210, 210, PI*-0.4, PI*1.0);

stroke(#3B3B39);
strokeWeight(12);
strokeCap(SQUARE);
arc(d, y, 180, 180, PI*-0.4, PI*1.0);





