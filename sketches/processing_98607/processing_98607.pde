
//Fred
size(200,200);
background(255);
smooth();

//set ellipses and rects to CENTER mode
ellipseMode(CENTER);
rectMode(CENTER);

//Draw Fred's body
fill(255,0,0);
ellipse(100,100,100,100);


//Draw Fred's eyes
fill(0);
ellipse(81,80,16,32);
ellipse(119,80,16,32);

//Draw Fred's legs
line(90,150,70,170);
line(115,149,130,175);

//Draw Fred's arms
line(50,100,15,80);
line(150,100,195,85);

//Draw Fred's smile
ellipse(100,120,20,20);

//Draw Fred's hair
line(100,50,100,30);
line(110,50,115,30);
line(90,50,85,30);
fill(0,255,0);





