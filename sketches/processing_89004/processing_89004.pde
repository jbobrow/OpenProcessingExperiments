
void setup()
{
  size(100,100);
  background(0);
}

void draw()
{
  if(mouseX<=40&&mouseX;>=60&&mouseY;<=40&&mouseY;>=60);
  {
    fill(252,122,0);
    ellipse(50,50,100,100);
    fill(0);
    triangle(30,30,20,40,40,40);
    triangle(70,30,60,40,80,40);
    rect(50,75,10,10);
    rect(35,65,10,10);
    rect(45,75,10,10);
    rect(55,65,10,10);
    rect(65,75,10,10);
  }
  
    if(mouseX>=40&&mouseX;<=60&&mouseY;>=40&&mouseY;<=60)
  {
    fill(255);
    rect(0,0,100,100);
  }
}
