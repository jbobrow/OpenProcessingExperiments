int count = 0;
void draw() {
  for (int X = 0; X < x+1; X+=1) {
    for (int Y = 0; Y < y+1; Y+=1) {
      if (X == 0 && Y == 0) {
        new_world[X][Y]= ((0.98*world[X][Y])+((0.0012*world[X][Y+1])+(0.0016*world[X+1][Y+1])+(0.0120*world[X+1][Y])));
      } 
      else if (X == 0 && Y == y) {
        new_world[X][Y]= ((0.98*world[X][Y])+((0.0120*world[X+1][Y])+(0.0016*world[X+1][Y-1])+(0.0012*world[X][Y-1])));
      } 
      else if (X == x && Y == y) {
        new_world[X][Y]= ((0.98*world[X][Y])+((0.0012*world[X][Y-1])+(0.0008*world[X-1][Y-1])+(0.0008*world[X-1][Y])));
      } 
      else if (X == x && Y == 0) {
        new_world[X][Y]= ((0.98*world[X][Y])+((0.0008*world[X-1][Y])+(0.0008*world[X-1][Y+1])+(0.0012*world[X][Y+1])));
      } 
      else if (X == 0) {
        new_world[X][Y]= ((0.98*world[X][Y])+((0.0012*world[X][Y+1])+(0.0016*world[X+1][Y+1])+(0.0120*world[X+1][Y])+(0.0016*world[X+1][Y-1])+(0.0012*world[X][Y-1])));
      } 
      else if (Y == y) {
        new_world[X][Y]= ((0.98*world[X][Y])+((0.0120*world[X+1][Y])+(0.0016*world[X+1][Y-1])+(0.0012*world[X][Y-1])+(0.0008*world[X-1][Y-1])+(0.0008*world[X-1][Y])));
      } 
      else if (X == x) {
        new_world[X][Y]= ((0.98*world[X][Y])+((0.0012*world[X][Y-1])+(0.0008*world[X-1][Y-1])+(0.0008*world[X-1][Y])+(0.0008*world[X-1][Y+1])+(0.0012*world[X][Y+1])));
      } 
      else if (Y == 0) {
        new_world[X][Y]= ((0.98*world[X][Y])+((0.0008*world[X-1][Y])+(0.0008*world[X-1][Y+1])+(0.0012*world[X][Y+1])+(0.0016*world[X+1][Y+1])+(0.0120*world[X+1][Y])));
      } 
      else {
        new_world[X][Y]= ((0.98*world[X][Y])+((0.0008*world[X-1][Y])+(0.0008*world[X-1][Y+1])+(0.0012*world[X][Y+1])+(0.0016*world[X+1][Y+1])+(0.0120*world[X+1][Y])+(0.0016*world[X+1][Y-1])+(0.0012*world[X][Y-1])+(0.0008*world[X-1][Y-1])));
      }
    }
  }

  background(0);
  world = new_world;
  if (count%5 == 0) {
    for (int B = 0; B < 10; B+=1) {
      if (heat[B][0] == 0) {
        if (int(random(100)) < 1) {
          heat[B][1] = int(random(y));
          heat[B][0] = 1;
        }
      } 
      else if (heat[B][2] <10) {
        world[49][(heat[B][1])] = world[49][(heat[B][1])] + 10;
        heat[B][2]+=1;
      } 
      else if (heat[B][2] <30) {
        world[49][(heat[B][1])] = world[49][(heat[B][1])] + 30;
        heat[B][2]+=1;
      } 
      else if (heat[B][2] <50) {
        world[49][(heat[B][1])] = world[49][(heat[B][1])] + 60;
        heat[B][2]+=1;
      } 
      else if (heat[B][2] <70) {
        world[49][(heat[B][1])] = world[49][(heat[B][1])] + 100;
        heat[B][2]+=1;
      } 
      else if (heat[B][2] <130) {
        world[49][(heat[B][1])] = world[49][(heat[B][1])] + 150;
        heat[B][2]+=1;
      } 
      else if (heat[B][2] <150) {
        world[49][(heat[B][1])] = world[49][(heat[B][1])] + 100;
        heat[B][2]+=1;
      } 
      else if (heat[B][2] <170) {
        world[49][(heat[B][1])] = world[49][(heat[B][1])] + 60;
        heat[B][2]+=1;
      } 
      else if (heat[B][2] <190) {
        world[49][(heat[B][1])] = world[49][(heat[B][1])] + 30;
        heat[B][2]+=1;
      } 
      else if (heat[B][2] <200) {
        world[49][(heat[B][1])] = world[49][(heat[B][1])] + 10;
        heat[B][2]+=1;
      } 
      else if (heat[B][2] == 200) {
        heat[B][2] = 0;
        heat[B][0] = 0;
      }
    }
  }

  for (int X = 0; X < x+1; X+=1) {
    for (int Y = 0; Y < y+1; Y+=1) {
      if (int(world[Y][X])<50) 
      {
        fill(0);
        rect(10*X, 10*Y, 9, 9);
      } 
      else if (int(world[Y][X])<150) 
      {
        fill((int(256*(world[Y][X]-50)/50)), 0, 0);
        rect(10*X, 10*Y, 9, 9);
      } 
      else if (int(world[Y][X])<350) 
      {
        fill(255, (int(256*(world[Y][X]-150)/200)), 0);
        rect(10*X, 10*Y, 9, 9);
      } 
      else if (int(world[Y][X])<1750) 
      {
        fill(256-(int(256*(world[Y][X]-450)/1250)), 256-(int(256*(world[Y][X]-450)/1250)), 0);
        rect(10*X, 10*Y, 9, 9);
      }     
      else if (int(world[Y][X])<15750) 
      {
        fill(0, 0, (int(256*(world[Y][X]-1750)/3000)));
        rect(10*X, 10*Y, 9, 9);
      } 
      else {
        fill(0, (int(256*(world[Y][X]-15750)/10000)), 256-(int(256*(world[Y][X]-15750)/10000)));
        rect(10*X, 10*Y, 9, 9);
      }
      //fill(0);
      //text(str(world[Y][X]), 50*X, 50*Y, 40, 40);
    }
  }
  count+=1;
} 


