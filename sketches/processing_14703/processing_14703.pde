
void setup() {
size(200,200);
background(255);
int i;
int x;
}

void draw() {
  for ( i=20; i <= 180; i = i + 10) {
    for ( x=20; x <=180; x = x + 7) {
      if(x=x+5){
        line(i,x,i+(x*0.04),x);
    }
    
    }
    }
    }
    
    
 
    
                
                                
