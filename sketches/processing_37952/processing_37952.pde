
 void setup(){
size(450,450);
background(255);
noStroke();
smooth();
}

void draw(){
  for(int i=0;i<100;i++){
    drawX(int(random(255)),int(random(255)),int(random(255)),int(random(width)),int(random(height)));
}
}

void drawX(int c1,int c2,int c3,int x,int y){
fill(c1,c2,c3,40);
ellipse(x,y,60,60);
}
           
                
