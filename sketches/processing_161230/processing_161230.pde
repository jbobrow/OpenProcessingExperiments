
//Chris Baio

size(200,200); //size of canvas
background(255); //canvas color

smooth();
ellipseMode(CENTER);
rectMode(CENTER);

stroke(0, 255, 0); //outline color of body 
fill(10, 50, 200); //body color of Zoog
rect(100,100,20,100); //body size

fill(18, 90, 200); //color of head
ellipse(100, 60, 60, 60); //size of head

fill(0); //color of eyes
ellipse(81, 60, 16, 16); //left eye
ellipse(119, 60, 16, 16); //right eye

stroke(255, 0, 0); //color of legs
line(90, 150, 80, 160); //left leg
line(110, 150, 120, 160); //right leg









