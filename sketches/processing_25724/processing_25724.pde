void draw(){  
  smooth();
  if(minute()%3==0 && second()==0){        //3分毎に背景書き換え、3分の分＝０かつ秒＝０
    background(0,0,col_wb);
  }
  col_h++;
  if(col_h==100){
    col_h=0;}
  if(second()==0){
    col_b=2.5*int(random(2,40));}
  if(second()==30){
    col_s=2.5*int(random(2,40));}
  stroke(col_h,col_s,col_b);
  
  s=second()/60.0*TWO_PI;         //秒針の角度計算  
  m=minute()/60.0*TWO_PI+s/60;    //分針の角度計算  
  h=(hour()%12)/12.0*TWO_PI+m/12; //時針の角度計算  

  pushMatrix(); 
  float xx=0;
  float yy=0;
  if(minute()%6==0){xx=width/4-75;yy=height/3-75;}
  if(minute()%6==1){xx=width*3/4+75;yy=height*2/3;}
  if(minute()%6==2){xx=width/2;yy=height/3-75;}
  if(minute()%6==3){xx=width/4-75;yy=height*2/3-45;}
  if(minute()%6==4){xx=width*3/4+75;yy=height/3-10;}
  if(minute()%6==5){xx=width/2;yy=height*2/3-20;}
  translate(xx,yy);  //画面の中心へずらす。

  pushMatrix();
  rotate(m);        //分針の回転角度
  strokeWeight(4);  //分針の太さを3ピクセルに設定  
  line(0,-40,0,-40); //分針の描画  
  popMatrix();  
  pushMatrix();  
  rotate(h);        //時針の回転角度  
  strokeWeight(10);  //時針の太さを5ピクセルに設定  
  line(0,-35,0,-35); //時針の描画  
  popMatrix(); 
  popMatrix();
  
  float x=0;  float y=0;
  if(minute()%6==0){x=random(width/4-80,width/4-70);y=random(height/3-70,height/3-80);}
  if(minute()%6==1){x=random(width*3/4+70,width*3/4+80);y=random(height*2/3-2,height*2/3+3);} 
  if(minute()%6==2){x=random(width/2-5,width/2+5);y=random(height/3-80,height/3-70);}
  if(minute()%6==3){x=random(width/4-80,width/4-70);y=random(height*2/3-50,height*2/3-40);}  
  if(minute()%6==4){x=random(width*3/4+70,width*3/4+80);y=random(height/3-30,height/3-20);}
  if(minute()%6==5){x=random(width/2-5,width/2+5);y=random(height*2/3-23,height*2/3-17);}
  
  translate(x,y);  //画面の中心へずらす。 
  strokeWeight(1);     //線の太さを1ピクセルにする  
  pushMatrix();
  rotate(s);     //秒針の回転角度
  strokeWeight(0.8);
  line(0,-50,0,random(-145,-115)); //秒針の描画 
  popMatrix();
  
  for(int n=0;n<=450;n++){
   if(tex_color[n]==color (col_h,col_s,col_b)){
     pushMatrix();
     song.rewind();
     song.play();
     colorMode(HSB, 100);
      pushMatrix();
        textFont(font1,11);
        txt=tx[n];
        translate(-100*cos(s),-100*sin(s),0);
        rotate(s);
        fill(tex_color[n]);
        text(txt,1,-170);
        popMatrix();
        pushMatrix();
        textFont(font2,14);
        txt2=tx2[n];
        translate(-100*cos(s),-100*sin(s),0);
        rotate(s);
        fill(tex_color[n]);
        text(txt2,1,-155);
        popMatrix();
   }
  }
}


