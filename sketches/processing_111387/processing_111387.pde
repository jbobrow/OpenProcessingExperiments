
size(500,500);
background(255);

for(int i=0; i<510; i=i+70){
  for(int j=0; j<510; j=j+70){
    
    
    strokeWeight(4);
    fill(26,98,100, 200);
    ellipse(i+10,j+10, 50,50);
    
    strokeWeight(4);
    fill(26,98,100);
    ellipse(i,j, 50,50);
    
    strokeWeight(6);
    fill(240,136,10);
    ellipse(i-40,j-35, 20,20);
    
    
    strokeWeight(4);
    fill(26,98,211);
    ellipse(i,j, 30,30);
    
   

    
  }
  
}
    
 


