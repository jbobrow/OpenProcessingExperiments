
size (500, 500);
background (147,150,152);
smooth();
noFill();
  
for(int w=20; w<= width+width; w+=0.23*w)
{
  stroke (255);
  strokeWeight (12);
  ellipse(width/2,1*height/4, w,w);
}
 
for(int w=50; w<= width+width; w+=0.6*w)
{
  stroke (0);
  strokeWeight (4);
  ellipse(width/7+0,3*height/4+15, 1.01*w,1.01*w);
  
}



