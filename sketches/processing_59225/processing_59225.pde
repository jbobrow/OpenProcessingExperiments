
size(400,400);
background(#25282E);
int x=100;
int y=150;
strokeWeight(10);
stroke(#FAFAFA);
line(x+100, y, 2*x, 2*y+100);
fill(#EB1515);
noStroke();
ellipse(2*x, y, 2*x+70, y+70);
fill(#035AFC);
ellipse(2*x, y, 2*x+50, y+50);
fill(#FC03DB);
ellipse(2*x, y, 2*x, y);
strokeWeight(3);
fill(#DFF705);
ellipse(2*x, y, 2*x-50, y-20);
fill(#FC03DB);
ellipse(x+100, y, x, y-50); 
fill(#035AFC);
ellipse(x+100, y, x/2, y-80); 
fill(#FC03DB);
ellipse(x+100, y, x/4, y-200); 

