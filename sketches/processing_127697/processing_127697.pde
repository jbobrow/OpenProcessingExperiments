
int counter;

void setup() {  //setup function called initially, only once
  size(500, 500);
  background(255);  //set background white
smooth ();
}

void draw() {  //draw function loops 
ellipse (210, 120, 50, 50); //red
fill (0,0,0);
ellipse (220, 150, 50, 50); //black
fill (0, 0, 255);
ellipse  (210, 190, 50, 50); //blue
fill (0, 255, 0); //green
ellipse (180, 160, 50, 50); //green
fill (255, 0, 0);
ellipse (200, 120, 50, 50);
fill (100, 100, 0);



}
