
void setup () {
size(400,400);
background (#077142);
}
void draw () {

  background(#F50C0C);
ellipseMode(CENTER);
rectMode(CENTER);


fill(150);
rect(200,200,40,200);

fill(255);
ellipse(mouseX,mouseY,120,120);


fill(0);
ellipse(172,140,32,60);
ellipse(220,140,32,60);


line(180,300,190,320);
line(220,300,240,320);
}
