void setup()
{
  
  slen = xpm[0].length();
  sheight = xpm.length;
  size(slen,sheight);
  fill(0,0,0,16);
  noStroke();
}

int scale = 1;
String s = null;
int scatterX = 0;
int scatterY = 0;
int slen = 0;
int sheight = 0;
int userThresh = 40;

void draw()
{
  background(255);
  for(int i = 0; i < sheight; i++)
  {
    s = xpm[i];
    for(int j = 0; j < slen; j++)
    {
      if( s.charAt(j) == "." )
      {
        int userDist = (int)dist( mouseX, mouseY, (j*scale), (i*scale));
        if( userDist < userThresh  )
        {
          userDist = userThresh - userDist;
          scatterX = (int)random(-1*userDist,userDist);
          scatterY = (int)random(-1*userDist,userDist);
        }
        else
        {
          scatterX = (int)random(-2,2);
          scatterY = (int)random(-2,2);
        }
        
        rect((j*scale)+scatterX,(i*scale)+scatterY,3,3);
      }
    }
  }
}
