  }

}
void draw(){
  background(0);
  stroke(255);
  
  for(int L =0; L < 25; L++)
  { 
    //off set added to each letter
    // ox = (L%8)*40;
    //  oy = (L/8)*40;
    
    
    pushMatrix();
    translate(55*(L%8), (L/8)*80);
    scale(.2,.2);
   
   
    
    float weight = 4.0;
    strokeWeight(weight);
    noFill();
    beginShape();
    for(int j =0; j<21; j++){
 // pushMatrix();
    //rotateY(y[0]);
   //rotateZ(z[0]);
   //rotateX(x[0]);
             currentLetter[j][0] = data[L][j][0];
        currentLetter[j][1] = data[L][j][1];
      float dx = random(-5,5);
      float dy = random(-5,5);
      
      curveVertex(currentLetter[j][0]+dx,currentLetter[j][1]+dy);
    
    }
    endShape();
    //popMatrix();
    popMatrix();
  }
}

