
void setup() {
  size(250,250);
  background(255);
}

void draw() {
  background(255);
  smooth();
  fill(102,45,145);
  stroke(102,45,145);
  ellipse(125,100,130,140);
  fill(0);
  stroke(0);
  strokeWeight(2);
  rectMode(CENTER);
  rect(125,120,50,25);
  fill(255);
  stroke(255);
  strokeWeight(1);
  triangle(101,110,117,110,109,120);
  triangle(117,110,133,110,125,120);
  triangle(133,110,149,110,141,120);
  fill(255);
  stroke(255);
  ellipse(110,78,12,15);
  ellipse(140,78,12,15);
  fill(0);
  stroke(0);
  ellipse(110,80,10,10);
  ellipse(140,80,10,10);
  stroke(102,45,145);
  strokeWeight(10);
  line(105,165,100,230);
  line(145,165,150,230);
  line(65,100,50,150);
  line(185,100,200,150);
