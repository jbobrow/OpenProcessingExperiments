
void setup()
{
  size(500,400);
  background(0);
}

void draw()
{  
  stroke(255);
  strokeWeight(1);
  strokeCap(ROUND);
  
  line(100,350,400,350);
  line(100,350,250,80);
  line(400,350,250,80);

  line(250,80,250,175);
  line(250,175,100,350);
  line(250,175,400,350);

  line(250,80,200,290);
  line(250,80,300,290);
  
  line(200,290,100,350);
  line(300,290,400,350); 
  
  line(100,350,250,310);
  line(400,350,250,310);
  
  line(250,310,250,275);
  
  line(200,290,250,275);
  line(300,290,250,275);
  line(200,290,250,310);
  line(300,290,250,310);
  
  line(230,220,270,220);
  
  line(220,211,230,220);
  line(270,220,280,211);

  line(230,220,250,275);
  line(270,220,250,275);
  
  line(270,220,250,175);
  line(230,220,250,175);
  
  line(270,220,300,290);
  line(230,220,200,290);
  
  ellipse(250,80,10,10);
  ellipse(250,175,10,10);
  ellipse(220,211,10,10);
  ellipse(230,220,10,10);
  ellipse(270,220,10,10);
  ellipse(280,211,10,10);
  ellipse(200,290,10,10);
  ellipse(250,275,10,10);
  ellipse(300,290,10,10);
  ellipse(250,310,10,10);
  ellipse(100,350,10,10);
  ellipse(400,350,10,10);
}
