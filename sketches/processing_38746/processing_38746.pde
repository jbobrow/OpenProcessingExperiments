
void setup(){
  
 size(600,400);
 background(150); 
}

void draw(){
  
   fill(0,237,247);
   stroke(247,70,0);
   ellipse(mouseX,mouseY,30,30);
   
   fill(247,70,0);
   stroke(0,237,247);
   ellipse(mouseX+100,mouseY+100,120,120);
   
      fill(0,237,247);
   stroke(247,70,0);
   ellipse(mouseX+170,mouseY+120,80,80);

 }
 
