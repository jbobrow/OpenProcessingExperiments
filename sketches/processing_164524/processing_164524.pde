
void setup(){
// sets the canvas size to 750pi by 750pi  
  size(750,750);
// sets back ground color to a shade of black  
  background(255);
// sets frame rate to 60 frames per second  
  frameRate(60);
}
  void draw(){
    fill(200, 10, 30);
    noStroke();
    rect(180, 500, 200, 200);
    fill(200, 10, 30);
    noStroke();
    rect(280, 350, 100, 100);
    fill(200, 10, 30);
    noStroke();
    rect(330, 250, 50, 50);
    fill(200, 10, 30);
    noStroke();
    rect(355, 175, 25, 25);
   
    fill(170, 30, 50);
    noStroke();
    rect(400, 500, 200, 200);
    fill(170, 30, 50);
    noStroke();
    rect(400, 350, 100, 100);
    fill(170, 30, 50);
    noStroke();
    rect(400, 250, 50, 50);
    fill(170, 30, 50);
    noStroke();
    rect(400, 175, 25, 25);
  }
  


