
size(101,101);

stroke(0);//colour of the outlines
fill(0);//black fill

noStroke();//no stroke colour
noFill();//no fill
fill(100,48,48,50);//fill(red, green, blue, transparancy);

rect(0,0,50,50);//black rectangle coordinates
fill(255);//white fill
rect(0,50,50,50);//white rectangle coordinates bottom left
rect(50,0,50,50);//white rectangle coordinates top right
fill(150);//grey rectangle bottom right
rect(50,50,50,50);//no fill rectangle coordinates

hue(200);//0-360
saturation(50);//0-100
brightness(50);//0-100

