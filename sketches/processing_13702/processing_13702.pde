
class Color {
  int r,g,b;

  Color(int r,int g,int b) {
    this.r = r;
    this.g = g;
    this.b = b;
  }

  int getRed() { 
    return r;
  }
  int getGreen() { 
    return g;
  }
  int getBlue() { 
    return b;
  }
  void setRed(int r) {
    this.r = r;
  }
  void setGreen(int g) {
    this.g = g;
  }
  void setBlue(int b) {
    this.b = b;
  }
}

