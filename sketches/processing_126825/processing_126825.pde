
/** Draws points on display to create Gemini constellation
  * By Julia Nara
  */

size(300, 500);

// Pollux
strokeWeight(15);
point(36, 100);

// Wasat
strokeWeight(5);
point(70, 220);

strokeWeight(5);
point(115, 250);

// Alhena
strokeWeight(7);
point(180, 325);

strokeWeight(5);
point(150, 360);

// Castor
strokeWeight(10);
point(80, 70);

strokeWeight(6);
point(175, 210);

strokeWeight(7);
point(225, 255);

strokeWeight(5);
point(245, 262);

