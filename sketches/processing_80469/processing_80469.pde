
//Size
size(600, 600);

//Couleur de fond blanche
background(250);

//Contour orange du rectangle vert
stroke(#FA7223);

//Couleur rectangle verte
fill(#C7FAE3);

//Rectangle vert
rect(50, 50, 500, 500);

//Pas de contour pour rectangle #1 & #2
noStroke();

//Couleur orange pour Rectangle #1
fill(#FA7223);

//Rectangle orange #1
rect(250, 250, 100, 100);

//Couleur orange translucide pour Rectangle #2
fill(255, 204, 51, 127);

//Rectangle orange translucide #2
rect(280, 280, 100, 100);

//Contour noir du quadrilatère sans remplissage
stroke(50);

//Pas de remplissage pour quadrilatère
noFill();

//Quadrilatère contour noir sans remplissage
quad(200, 180, 240, 260, 420, 300, 250, 400);



