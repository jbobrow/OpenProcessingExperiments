
//EMOTION EVOKER
//MILTON SUKHABUT, Sad Emotion, PS_1

void setup(){
  size(500,500);
  stroke(0);
  smooth();
}

void draw (){
    background(130);
    fill(204,204,255);
    strokeWeight(3);
    
    ellipse(250 ,240, 300, 300);
    fill(0);
    
    ellipse(250, 210, 30, 70);
    fill(0, 102,255);
    
    ellipse(230, 260, 10, 30);
    fill(0);
    
    ellipse(280, 320, 60, 10);
      
    ellipse(300, 210, 30, 70);
    fill(155);
    
    
}
  

