
//Line exercise 
size (600,200);
smooth ();
background (#9f9694);

stroke(#e2e1dc);
strokeWeight(3);
line (100, 50, 500, 50);
line (100, 150, 500, 150); 

stroke(#791f33);
strokeWeight(8);
line (100, 50, 100, 150);  //drawing order matters 
line (100, 50, 200, 150);  
line (100, 50, 300, 150);  
line (100, 50, 400, 150);  

line (200, 50, 500, 150);  
line (300, 50, 500, 150);  
line (400, 50, 500, 150);  
line (500, 50, 500, 150);  
