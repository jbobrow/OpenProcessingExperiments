

void setup() 
{
size(696, 696);
background(255); //set background white
colorMode(RGB); //set colors to Hue, Saturation, Brightness mode
}
void draw()
{
stroke(0);
fill(192,192,192);
rect(100, 100, 240, 240);

fill(0,0, 0);
ellipse(600, 600, 360, 360)

fill(255,255,255);
triangle(30, 70, 58, 20, 86, 70 );


}
 
