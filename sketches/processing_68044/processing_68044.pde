void setup()
{
  size(400,400);
  background(#FFFFFF);
  noFill(); 
  smooth();
  strokeWeight(4);
}

void draw ()
{
  background(#FFFFFF); 
  stroke(#3299CC);
  ellipse(150,150,70,70);
  
  stroke(#000000);
  ellipse(210,150,70,70);
  
  stroke(#FF0000);
  ellipse(270,150,70,70);
  
  stroke(#FFFF00);
  ellipse(190,200,70,70);
  
  stroke(#00FF00);
  ellipse(250,200,70,70);
  
} 

