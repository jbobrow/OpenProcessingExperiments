
void setup() {
  frameRate(30);
  size(500, 500);
  smooth();
  background(255);
}

void draw() {

  for (int x = 0; x < 500; x += 5) {
    strokeWeight(random(20)); // Increase thickness
    fill(255);
    if (mousePressed == true) {
      stroke(random(255));
      ellipse(mouseX, mouseY, 10, 10);
    }
  }
}
