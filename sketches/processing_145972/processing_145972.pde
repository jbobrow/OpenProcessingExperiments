class Button {
  boolean set;
  int x, y;
  int buttonW, buttonH;
  int subButtonW, subButtonH;
  boolean overButton = false;
  color buttonC;
  String letterGrade = "";
  float credit = 0.0;
  float gradeVal;
  int clickCount = 0;
  
  Button(int inX, int inY, int tempButtonW, int tempButtonH, color tempButtonC) {
    set = false;
    x = inX;
    y = inY;
    buttonW = tempButtonW;
    buttonH = tempButtonH;
    buttonC = tempButtonC;
  }
  Button(int inX, int inY, int tempButtonW, int tempButtonH, color tempButtonC, String tempLetterGrade, float tempCredit, float tempGradeVal, boolean tempSet) {
    set = false;
    x = inX;
    y = inY;
    buttonW = tempButtonW;
    buttonH = tempButtonH;
    buttonC = tempButtonC;
    letterGrade = tempLetterGrade;
    credit = tempCredit;
    gradeVal = tempGradeVal;
    set = tempSet;
  }
  
  void display() {
    fill(buttonC);
    rect(x, y, buttonW, buttonH, 5);
    fill(0);
    textSize(14);
    text(letterGrade,x+3,y+18);
    hoverButton();
  }
  
  // Handle mouse actions
  void hoverButton() {
    overButton = mouseX > x && mouseX < x+buttonW && mouseY > y && mouseY < y+buttonH;
    if(overButton == true && mousePressed == true && (mouseButton == LEFT)) {
      clickCount++;
      if(clickCount == 1){
        assignGrade(" A");
      } else if (clickCount == 2){
        assignGrade("A-");
      } else if (clickCount == 3){
        assignGrade("B+");
      } else if (clickCount == 4){
        assignGrade(" B");
      } else if (clickCount == 5){
        assignGrade("B-");
      } else if (clickCount == 6){
        assignGrade("C+");
      } else if (clickCount == 7){
        assignGrade(" C");
      } else if (clickCount == 8){
        assignGrade("C-");
      } else if (clickCount == 9){
        assignGrade("D+");
      } else if (clickCount == 10){
        assignGrade(" D");
      } else if (clickCount == 11){
        assignGrade("D-");
      } else if (clickCount == 13){
        assignGrade(" F");
      } else if (clickCount == 14){
        assignGrade("P");
      }else if (clickCount == 15){
        assignGrade("R");
      }
      //stroke(200,255,0);
    } else if (overButton == true && mousePressed == true && (mouseButton == RIGHT)){
      clickCount--;
      if(clickCount == 1){
        assignGrade(" A");
      } else if (clickCount == 2){
        assignGrade("A-");
      } else if (clickCount == 3){
        assignGrade("B+");
      } else if (clickCount == 4){
        assignGrade(" B");
      } else if (clickCount == 5){
        assignGrade("B-");
      } else if (clickCount == 6){
        assignGrade("C+");
      } else if (clickCount == 7){
        assignGrade(" C");
      } else if (clickCount == 8){
        assignGrade("C-");
      } else if (clickCount == 9){
        assignGrade("D+");
      } else if (clickCount == 10){
        assignGrade(" D");
      } else if (clickCount == 11){
        assignGrade("D-");
      } else if (clickCount == 13){
        assignGrade(" F");
      } else if (clickCount == 14){
        assignGrade("P");
      }else if (clickCount == 15){
        assignGrade("R");
      }
    }
  }

  void assignGrade(String grade) {
    letterGrade = grade;
    set = true;
    if (letterGrade == " F"){
      gradeVal = .0;
      credit = 0;
      buttonC = color(230,180,180);
    } else if (letterGrade == " A"){
      gradeVal = 4.0;
      credit = 0.25;
      buttonC = 200;
    } else if (letterGrade == "A-"){
      gradeVal = 3.666;
      credit = 0.25;
      buttonC = 200;
    } else if (letterGrade == "B+"){
      gradeVal = 3.333;
      credit = 0.25;
      buttonC = 200;
    } else if (letterGrade == " B"){
      gradeVal = 3.00;
      credit = 0.25;
      buttonC = 200;
    } else if (letterGrade == "B-"){
      gradeVal = 2.666;
      credit = 0.25;
      buttonC = 200;
    } else if (letterGrade == "C+"){
      gradeVal = 2.333;
      credit = 0.25;
      buttonC = 200;
    } else if (letterGrade == " C"){
      gradeVal = 2.00;
      credit = 0.25;
      buttonC = 200;
    } else if (letterGrade == "C-"){
      gradeVal = 1.666;
      credit = 0.25;
      buttonC = 200;
    } else if (letterGrade == "D+"){
      gradeVal = 1.333;
      credit = 0.25;
      buttonC = 200;
    } else if (letterGrade == " D"){
      gradeVal = 1.00;
      credit = 0.25;
      buttonC = 200;
    } else if (letterGrade == "D-"){
      gradeVal = 0.666;
      credit = 0.25;
      buttonC = 200;
    } else if (letterGrade == "P"){
      gradeVal = 0.00;
      set = false;
      credit = 0.25;
      buttonC = 200;
    }else if (letterGrade == "R"){
      set = false;
      gradeVal = 0.0;
      credit = 0.0;
      letterGrade = "";
      buttonC = 200;
    }
  }
}

