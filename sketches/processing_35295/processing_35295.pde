
int x=300;

size(300, 300);

float p=5.0;

fill(252, 0, 0);
ellipse(x/2, x/2, 3*p, 3*p);

fill(252, 240, 0);
ellipse(x/2+100, x/2, 30, 30);

fill(41, 252, 0);
rectMode(CENTER);
rect(x/p, x/2, 80, 80);

stroke(252, 101, 0);
line(.6*x, x/2+15, .6*x, x/2-15);

fill(0, 35, 252);
stroke(1);
triangle(x/3, x/2-2*p, x/3-2*p, x/2+2*p, x/3+2*p, x/2+2*p);










