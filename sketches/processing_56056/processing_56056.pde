
void setup() {
  size(500, 500);
  smooth();
  noStroke();
}
void draw() { 
  fill(0150, 100, 200);
  ellipse(mouseX, mouseY, 14, 14);
  stroke(10,50,40);
for (int x=mouseX; x <= height-20; x += 60) {
for (int y=mouseY; y <= width-20; y += 60) {
  line(250, 250, mouseX, mouseY);
   if (mousePressed == true) 
   
   background(150, 100, 200);
}
}
}

