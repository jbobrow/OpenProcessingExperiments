
//operation occurs once and is stagnant
void setup(){
  //the size of the frame
  size(500,500);
  //background color in void setup allows for "drawing" when mouseX is involved
  background(0,60,100);
}
//operations "loop"
void draw(){
  //creates a fill for the following codes
  fill(50,100,0);
  //creates a rectangle as (x,y,width,height)
  rect(100,100,200,200);
  rect(110,110,180,180);
  rect(120,120,160,160);
  rect(130,130,140,140);
  rect(140,140,120,120);
  rect(150,150,100,100);
  rect(160,160,80,80);
  rect(170,170,60,60);
  rect(180,180,40,40);
  rect(190,190,20,20);
  rect(200,200,0,0);
}


