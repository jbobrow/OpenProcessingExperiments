
// Starting with the third vertex, connects each
// subsequent vertex to the previous two
beginShape(TRIANGLE_STRIP);
fill(0);
vertex(75, 30);
fill(20);
vertex(10, 20);
fill(20*2);
vertex(75, 50);
fill(20*3);
vertex(20, 60);
fill(20*4);
vertex(90, 70);
fill(20*5);
vertex(35, 85);
endShape();
