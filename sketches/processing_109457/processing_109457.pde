  rectMode(CORNERS);
  fill(x, x, x);
  rect(310, 0, 250, 60);
  fill(x, 255, 255);
  rect(30, 80, 80, 160);
  fill(255, x, 0);
  rect(80, 190, 250, 240);
  line(140, 0, 140, 190);
  line(80, 160, 250, 160); 
  line(90, 240, 90, 320);
  line(130, 240, 130, 320);
  line(165, 240, 165, 320);
  line(235, 240, 235, 320);
  line(230, 240, 230, 320);
  fill(x, 0, x);
  rect(180, 355, 250, 400);
  fill(45, 135, x);
  rect(165, 320, 200, 355);
  fill(255, x, 255);
  rect(0, 320, 30, 400);
  line(310, 300, 400, 300);
