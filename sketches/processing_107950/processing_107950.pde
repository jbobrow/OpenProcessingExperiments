// HW_1_Initials

size(400, 400);
strokeWeight(1);


// White Rectangle
noStroke();
rect(0, 0, 400, 220);

// Start_sine
noStroke();
arc(5, 190, 30, 100, radians(0), radians(180), OPEN);
fill(205);
arc(33, 250, 40, 100, radians(210), radians(280));

// J
stroke(0);
arc(33, 250, 40, 100, radians(270), radians(330));
fill(255);
arc(60, 190, 28, 100, radians(1), radians(137));
noStroke();
fill(205);
arc(88, 200, 28, 100, radians(0), radians(360));
stroke(0);
fill(205);
arc(88, 200, 28, 100, radians(185), radians(275));

// Grey Rectangle
noStroke();
rect(60, 200, 280, 200);

// Redraw bottom of J
stroke(0);
fill(255);
arc(60, 190, 28, 100, radians(1), radians(148));

// Connector
noStroke();
fill(205);
arc(88, 200, 28, 100, radians(270), radians(360));
fill(255);
arc(116, 190, 28, 100, radians(0), radians(170));
fill(205);
arc(144, 200, 28, 100, radians(0), radians(360));

// W
stroke(0);
arc(144, 200, 28, 100, radians(270), radians(359));
fill(255);
arc(172, 190, 28, 100, radians(10), radians(171));
noStroke();
fill(205);
arc(200, 200, 28, 50, radians(0), radians(360));
stroke(0);
fill(205);
arc(200, 200, 28, 50, radians(181), radians(359));
fill(255);
arc(228, 190, 28, 100, radians(10), radians(170));
noStroke();
fill(205);
arc(256, 200, 28, 100, radians(0), radians(360));
stroke(0);
fill(205);
arc(256, 200, 28, 100, radians(181), radians(280));

// Connector
noStroke();
arc(256, 200, 28, 100, radians(270), radians(360));
fill(255);
arc(284, 190, 28, 100, radians(10), radians(170));
fill(205);
arc(312, 200, 28, 100, radians(0), radians(360));

// H
stroke(0);
arc(312, 200, 28, 100, radians(270), radians(359));
fill(255);
arc(340, 190, 28, 100, radians(40), radians(179));
fill(205);
arc(367, 250, 40, 100, radians(210), radians(330));
fill(255);
arc(395, 190, 30, 100, radians(88), radians(141));

// End
noStroke();
arc(395, 190, 30, 100, radians(40), radians(90));


