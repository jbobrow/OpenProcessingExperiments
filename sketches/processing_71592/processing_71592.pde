line (90, mouseY, 90, mouseY);
line (135, mouseY, 135, mouseY);
line (180, mouseY, 180, mouseY);
line (225, mouseY, 225, mouseY);
line (270, mouseY, 270, mouseY);
line (315, mouseY, 315, mouseY);
line (365, mouseY, 365, mouseY);
}
