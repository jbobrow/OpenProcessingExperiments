
class ParkDetails {

  // instance variables
  String cityState;
  String latitude;
  String longitude;
  int yearBuilt;

  // constructor
  ParkDetails( String cityState_, String latitude_, String longitude_, int yearBuilt_) {
    cityState = cityState_;
    latitude = latitude_; 
    longitude = longitude_;
    yearBuilt = yearBuilt_;
  }
}


