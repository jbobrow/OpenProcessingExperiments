
void setup(){
  size(450,250);
  smooth();
  background(255);
}

void draw(){
  strokeWeight(10);
  noFill();
  stroke(#4B7CC9);
  ellipse(100,100,100,100);
  stroke(#F7E450);
  ellipse(160,140,100,100);
  stroke(#050505);
  ellipse(220,100,100,100);
  stroke(#337C28);
  ellipse(280,140,100,100);
  stroke(#CB2929);
  ellipse(340,100,100,100);
}
