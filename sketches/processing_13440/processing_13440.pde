
void setup(){
  size(640,480);
  smooth();
}

void draw(){
  background(0,20,0);

  fill(255,255,0);
  noStroke();
  ellipse(320,240,60,60);

  noFill();
  stroke(255,127,0);
  strokeWeight(10);
  ellipse(320,240,80,80);

  stroke(235,117,0);
  ellipse(320,240,110,110);

  stroke(205,107,0);
  ellipse(320,240,140,140);

  stroke(185,97,0);
  ellipse(320,240,170,170);

  stroke(165,87,0);
  ellipse(320,240,200,200);
}





