
void setup(){
 
  size(750,750);

  background(24,184,240);

  frameRate(60);
}
   
void draw(){
  fill(61,216,124);
  noStroke();
  ellipse(250,200,200,200);//space between circles
  fill(114,108,188);//color of circles
  noStroke();
  ellipse(500,200,200,200);
  fill(61,216,124);
  noStroke();
  ellipse(500,500,200,200);
  fill(114,108,188);
  noStroke();
  ellipse(250,500,200,200);
}



