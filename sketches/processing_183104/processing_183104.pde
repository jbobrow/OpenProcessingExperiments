
// width, height
size(400, 200);

// r, g, b
background(0, 0, 255);

// x, y
// this is how processing draws a point
point(50, 20);
point(90, 90);

noStroke();

// yellow?
fill(255, 255, 0);
// x, y, width, height
rect(20, 40, 80, 80);

fill(0, 255, 0);
// x, y, width, height
ellipse(40, 60, 80, 80);

fill(0, 255, 255);
ellipse(120, 40, 100, 100);

// grayscale
stroke(150);
strokeWeight(10);
// x1, y1, x2, y2
line(0, 0, 400, 200);


