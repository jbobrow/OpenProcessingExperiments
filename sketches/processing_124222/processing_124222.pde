
//Snowman Made By Christian Rodenhouse
void setup()
{
  size(400,600);
  background(0,0,0);
}
void draw()
{
  frameRate(30);
  fill(255,255,255);
  ellipse(width/2, 495, 200, 200);
  fill(255, 255, 255);
  ellipse(width/2, 350, 150, 150);
  fill(255,255,255);
  ellipse(width/2, 240, 100, 100);
  fill(167,152,152);
  rect(135,185, 130, 15);
  fill(167,152,152);
  rect(155,110,90,90);
  fill(0,0,0);
  ellipse(width/2,535, 15,15);
  fill(0,0,0);
  ellipse(width/2,505, 15,15);
  fill(0,0,0);
  ellipse(width/2,475, 15,15);
  fill(255,255,255);
  fill(255,255,255,random(120));
  noStroke();
  ellipse(random(400),random(600),5,5);
  fill(0,0,0);
  ellipse(180,230,10,10);
  fill(0,0,0);
  ellipse(220,230,10,10);
  fill(255,128,0);
  triangle(width/2,270, 190,240, 210, 240);


  
  
  
  
}
