size(400,400);
  background(0,200,0);
  
float x=20;
float y=20;
float wd=400;
float ht=400;


fill(0,0,200);
beginShape();
 curveVertex(x-.5*wd, y+.2*ht);
 curveVertex(x+.5*wd, y+.2*ht);
 curveVertex(x+.8*wd, y+.4*ht);
 curveVertex(x+.5*wd, y+.6*ht);
 curveVertex(x+.2*wd, y+.7*ht);
 curveVertex(x+.4*wd, y+.8*ht);
 curveVertex(x+.8*wd, y+.8*ht);
 curveVertex(x+.5*wd, y+.6*ht);
 curveVertex(x+.3*wd, y+.4*ht);
 curveVertex(x+.5*wd, y+.2*ht);
 curveVertex(x+1.5*wd, y+.2*ht);
endShape();



