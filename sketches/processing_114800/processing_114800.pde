
void setup() {
  size(600,600);
  background(225);
  ellipse(width/2,height/2,200,200);
   fill(5);
}
 
void draw() {
   ellipse(width/2,height/2,200,200);
    background(255);
   

    if(mousePressed == true){
      
    ellipse(width/2,height/2,200,200);
    fill(5);
    background(23,11,175);
    fill(5);
    
}else{
   ellipse(width/2,height/2,200,200);
    fill(5);
  }
   
   
}



