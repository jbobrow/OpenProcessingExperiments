
void setup() {  
    size(500,500);
   background(250,58,254);
}

void draw() {
  fill(255,255,255);
  ellipse(250,150,100,100);
rect(200,200,100,100);  
fill(0,0,0);
ellipse(225,125,5,5);
fill(0,0,0);
ellipse(275,125,5,5);
fill(255,255,255);
strokeWeight(5);
}

 void mousePressed(){
  
 background(random(255), random(255), random(255));
 fill(255,255,255);
  ellipse(250,150,100,100);
rect(200,200,100,100);  
 fill(0,0,0);
ellipse(225,125,5,5);
fill(0,0,0);
ellipse(275,125,5,5);
fill(255,255,255);
strokeWeight(5);
 }
 
                
                
