
size(600,600);
strokeWeight(.3);
smooth();
background(0);

colorMode(RGB);

for (int x=0; x<=600; x+=100){
for (int y=0; y<=600; y+=5){
for (int z=0; z<=1000; z+=55){
stroke(100,0,255, 79);
line(x,y,z,z);
}}}

