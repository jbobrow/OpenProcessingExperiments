
size(500,500);
background(240);
smooth();

strokeWeight(1/2);

for(int i=0; i<510; i=i+10){
     
      stroke(#284A7E);
     line(500, i, 0, 500);  
 
     stroke(#008383);
     line(0, i, 500, 500);
     
     stroke(#008383);
     line(0, i, 250, 250);
     
     stroke(#284A7E);
     line(500, i, 250, 250);
    
 
 }


