
void setup()
{
  background(100);
  size(600,600);
}
void draw()
{
  background(100);
  noFill();
  stroke(0);
  fill(0);
  ellipse(mouseX,mouseY,220,220);
  fill(100);
  ellipse(mouseX,mouseY,160,160);
  fill(255);
  ellipse(mouseX,mouseY,180,180);
  fill(255,255,0);
  ellipse(mouseX,mouseY,200,200);
  
}
