
void setup()
{
  background(100);
  size(600,600);
  rect(150,80,55,55,20);
  fill(255,243,89);
  triangle(100,240,30,20,45,77);
}
void draw()
{
  background(100);
  noFill();
  stroke(0);
  fill(0);
  ellipse(mouseX,mouseY,420,220);
  fill(100,55,250);
  ellipse(mouseX,mouseY,360,180);

  ellipse(mouseX,mouseY,180,180);
 
  ellipse(mouseX,mouseY,200,200);

 
  
   
}


 
  
