
void setup () {
  size(600,400);
}

void draw (){
  background(#33CC33);
  ellipse(100, 200, 55, 55);
  fill(#CC33FF);
  ellipse(200, 200, 110, 110);
  fill(125);
  ellipse(400, 200, 165, 165);
  fill(170);
  ellipse(300, 200, 220, 220);
  fill(60);
  rect(30, 20, 55, 55);
  fill(#FF6699);
  line(100,100,500,100);
  stroke(125);
  ellipse(mouseX,mouseY, 50,50);
  
}
