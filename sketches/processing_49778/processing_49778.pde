
size(600,600);
noFill();
smooth();
strokeWeight(10);
strokeCap(SQUARE);

// PI = 3.14 = 180 degrees    
// radians(180) will give you a decimal point for the degree amount


arc(300,300,   100,100,   radians(30),  radians(80));
arc(300,300,   150,150,   radians(50),  radians(180));
arc(300,300,   200,200,   radians(30),  radians(80));
arc(300,300,   250,250,   radians(30),  radians(80));
arc(300,300,   300,300,   radians(30),  radians(80));

