
/** Create star-like design using points 
  * By Julia Nara
  */

size(500, 500);

// Center Point
strokeWeight(40);
point(250, 250);

// Line of points above center point
strokeWeight(15);
point(250, 200);
strokeWeight(10);
point(250, 150);
strokeWeight(5);
point(250, 100);
strokeWeight(2);
point(250, 50);

// Line of points below center point
strokeWeight(15);
point(250, 300);
strokeWeight(10);
point(250, 350);
strokeWeight(5);
point(250, 400);
strokeWeight(2);
point(250, 450);

// Line of points to left of center point
strokeWeight(15);
point(200, 250);
strokeWeight(10);
point(150, 250);
strokeWeight(5);
point(100, 250);
strokeWeight(2);
point(50, 250);

// Line of points to right of center point
strokeWeight(15);
point(300, 250);
strokeWeight(10);
point(350, 250);
strokeWeight(5);
point(400, 250);
strokeWeight(2);
point(450, 250);

// Top left diagonal of points
strokeWeight(15);
point(200, 200);
strokeWeight(10);
point(150, 150);
strokeWeight(5);
point(100, 100);
strokeWeight(2);
point(50, 50);

// Bottom left diagonal of points
strokeWeight(15);
point(300, 300);
strokeWeight(10);
point(350, 350);
strokeWeight(5);
point(400, 400);
strokeWeight(2);
point(450, 450);

// Top right diagonal of points
strokeWeight(15);
point(300, 200);
strokeWeight(10);
point(350, 150);
strokeWeight(5);
point(400, 100);
strokeWeight(2);
point(450, 50);

// Bottom right diagonal of points
strokeWeight(15);
point(200, 300);
strokeWeight(10);
point(150, 350);
strokeWeight(5);
point(100, 400);
strokeWeight(2);
point(50, 450);



