
float x,y,x2,y2;

PrintWriter output;

void setup() {
  size(630,320);
  background(51,51,51);
  strokeWeight(2);
  frameRate(2);
   //Create a new file in the sketch directory  
  //output = createWriter("positions.txt"); 
  //imagen();
  print("setup");
  copia();
}


void draw()
{
  estrella();
}


void estrella()
{
  background(51,51,51);
  float c=random(0,10);
  fill(255,255,0);
  stroke(255,255,0);
  strokeWeight(1);
  ellipse(random(0,490),random(0,49),c,c);
  copia();
}




void keyPressed() {
  
  if(key=='w')
    {
      output.flush(); // Writes the remaining data to the file
      output.close(); // Finishes the file
      exit(); // Stops the program
    }
  else
  if(key=='1')
    output.println("//1");
  else
  if(key=='2')
    output.println("//2");
  else
  if(key=='3')
    output.println("//3");
  else
  if(key=='4')
    output.println("//4");
    print("key");
  x2=0;
  y2=0;
}

void mouseClicked()
{
    point(mouseX,mouseY);
    x=mouseX;
    y=mouseY;
    //output.println("line("+x+","+y+","+x2+","+y2+");");
    //output.println("point("+x+","+y+");"); // Write the coordinate to the file
    //output.flush();
    x2=x;
    y2=y;
    print("clicked");
}




void imagen()
{
  PImage ghost;
  ghost=loadImage("volcanes4.jpg");
  image(ghost,0,0);
}


