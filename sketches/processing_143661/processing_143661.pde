void setup(){
  size(600,600);
  background(0);
  smooth();
  noLoop();
}

void draw(){
  
  stroke(0);
  strokeWeight(6);
noFill();

for (int i = -100; i < width; i = i + 20) {
 for (int j = -100; j < height; j = j +20) {  
 stroke(0);
   fill(255);
   rect(i,j,100,100);
   



} 

   
 }
}




