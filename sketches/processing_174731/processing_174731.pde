/*Aufgabenstellung
Zeichne ein Tier, z.B. den ausliegenden Pinguin */

size(300,300);
background(255,0,255);
fill(0,0,0);
strokeWeight(12);
stroke(0,0,0);
ellipse(150,150,200,250);
fill(255,255,255);
stroke(255,255,255);
ellipse(150,190,130,120);
fill(255,255,255);
stroke(255,255,255);
ellipse(140,80,30,70);
