
void setup() {
  size(400, 400);

  background(random(255));
}

void draw() {
  if (!mousePressed) {
    ellipse(random(400), random(400), 50, 70); 
    fill(random(255), random(255), random(255));
  }
}
void mousePressed() {
}

void keyPressed() {
  background(255);
}

if(mouseX = 300) {
fill(random(255), random(255), random(255));
background()
}

if(mouseY = 100) {
fill(random(255), random(255), random(255));
background()
}

