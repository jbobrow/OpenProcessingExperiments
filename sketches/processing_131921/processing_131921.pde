
// This is a symmetrical sketch containing fifty-four distinct objects total in a few shades of blue. There are 18 ellipses and 36 arcs, layered upon or adjacent to each other, reminiscent of targets.

// Setup
size(600, 600);
background(#020864);

// Ellipses
stroke(#8B95FF);
strokeWeight(10);
ellipse(100, 100, 80, 80);
ellipse(100, 300, 80, 80);
ellipse(100, 500, 80, 80);
ellipse(300, 100, 80, 80);
ellipse(300, 300, 80, 80);
ellipse(300, 500, 80, 80);
ellipse(500, 100, 80, 80);
ellipse(500, 300, 80, 80);
ellipse(500, 500, 80, 80);
fill(#020864);
ellipse(100, 300, 40, 40);
ellipse(300, 100, 40, 40);
ellipse(300, 500, 40, 40);
ellipse(500, 300, 40, 40);
noFill();
ellipse(100, 100, 40, 40);
ellipse(100, 500, 40, 40);
ellipse(300, 300, 40, 40);
ellipse(500, 100, 40, 40);
ellipse(500, 500, 40, 40);

// Arcs
stroke(#8BD1FF);
strokeWeight(8);
strokeCap(SQUARE);
noFill();
 // Set 1
arc(90, 90, 100, 100, PI, 3*PI/2);
arc(110, 90, 100, 100, 3*PI/2, 2*PI);
arc(90, 110, 100, 100, PI/2, PI);
arc(110, 110, 100, 100, 0, PI/2);
 // Set 2
arc(290, 90, 100, 100, PI, 3*PI/2);
arc(310, 90, 100, 100, 3*PI/2, 2*PI);
arc(290, 110, 100, 100, PI/2, PI);
arc(310, 110, 100, 100, 0, PI/2);
 // Set 3
arc(490, 90, 100, 100, PI, 3*PI/2);
arc(510, 90, 100, 100, 3*PI/2, 2*PI);
arc(490, 110, 100, 100, PI/2, PI);
arc(510, 110, 100, 100, 0, PI/2);
 // Set 4
arc(90, 290, 100, 100, PI, 3*PI/2);
arc(110, 290, 100, 100, 3*PI/2, 2*PI);
arc(90, 310, 100, 100, PI/2, PI);
arc(110, 310, 100, 100, 0, PI/2);
 // Set 5
arc(290, 290, 100, 100, PI, 3*PI/2);
arc(310, 290, 100, 100, 3*PI/2, 2*PI);
arc(290, 310, 100, 100, PI/2, PI);
arc(310, 310, 100, 100, 0, PI/2);
 // Set 6
arc(490, 290, 100, 100, PI, 3*PI/2);
arc(510, 290, 100, 100, 3*PI/2, 2*PI);
arc(490, 310, 100, 100, PI/2, PI);
arc(510, 310, 100, 100, 0, PI/2);
 // Set 7
arc(90, 490, 100, 100, PI, 3*PI/2);
arc(110, 490, 100, 100, 3*PI/2, 2*PI);
arc(90, 510, 100, 100, PI/2, PI);
arc(110, 510, 100, 100, 0, PI/2);
 // Set 8
arc(290, 490, 100, 100, PI, 3*PI/2);
arc(310, 490, 100, 100, 3*PI/2, 2*PI);
arc(290, 510, 100, 100, PI/2, PI);
arc(310, 510, 100, 100, 0, PI/2);
 // Set 9
