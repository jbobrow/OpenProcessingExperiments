    //Moon.
    smooth();
    shape(moon, moonx, moony, 760, 610);
  }

  //Press key to move moon.
  if (keyPressed && click == true && city == true) {
    moonbutton = true;

    //Button no longer active.
    click = false;
  }

  //Moon moves to top in half circle.
  if (moonbutton) {
    moonx = cos(radians(angle))*(radius);
    moony = (sin(radians(angle))*(radius))-100;
    angle = angle + 1;
  }

  //When moon reaches top, cirlcular motion stops.
  if (moonx > 0) {
    moonx = 0;
    moonbutton = !moonbutton;
    start = true;
  }

  //Moon drops with background.
  if (start) {
    if (moonx < height-25) {
      back=back+(abs(speed));
    }

    moony=moony+speed;

    speed = speed+gravity;


    //When background fills screen, all background movement stops.
    if (back > -2) {
      back = -2;

      //Moon will not be reset.
      ball = !ball;
    }

    //Moon bounce.
    if (moony>height-225) {
      moony=height-225;

      if (speed<.6) {
        speed=0;
      }
      else {
        speed = speed * -.6;
      }

      //End.
      if (speed == 0) {
        stroke(255);
        noSmooth();
        for (int end = 300; end < 470; end +=60) {
