
void setup(){
  
 background(255, 255, 255);
 size(400, 400);
 noFill();
 smooth(); 

}

void draw(){
  
  stroke(0,0,255);
  strokeWeight(4);   
  ellipse(100, 100, 50, 50);
  
  stroke(0,0,0);
  strokeWeight(4);   
  ellipse(160,100 , 50, 50);
  
  stroke(255,0,0);
  strokeWeight(4);   
  ellipse(220,100 , 50, 50);
  
  stroke(255,255,0);
  strokeWeight(4);   
  ellipse(130,130 , 50, 50);
  
  stroke(0,255,0);
  strokeWeight(4);   
  ellipse(190,130 , 50, 50);
  
  
}



