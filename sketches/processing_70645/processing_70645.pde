
//Original Grid size is 20x30

void setup(){
size(1000,1000);
background(128);
}
void draw() {
rectMode(CORNERS);
ellipseMode(CORNERS);
      //Face Shape
fill(128,200,128);
ellipse(130,1000-320,270,1000-80);
stroke(128);
fill(128);
rect(130,1000-140,270,1000-120);
ellipse(100,1000-170,160,1000-60);
ellipse(240,1000-170,300,1000-60);
        //eyes 
fill(0);
noStroke();
ellipse(140,1000-200,170,1000-180);
ellipse(230,1000-200,260,1000-180);
ellipse(170,1000-250,230,1000-230);
ellipse(190,1000-250,210,1000-230);
      //Pupils
fill(255);
ellipse(150,1000-200,160,1000-190);
ellipse(240,1000-200,250,1000-190);
ellipse(190,1000-250,210,1000-230);
      //Nostrils
fill(0);
ellipse(185,1000-170,195,1000-150);
ellipse(205,1000-170,215,1000-150);
      //line
stroke(0);
line(260,1000-280,260,1000-350);
line(260,1000-350,300,1000-350);
line(300,1000-350,300,1000-290);
line(140,1000-280,140,1000-350);
line(140,1000-350,100,1000-350);
line(100,1000-350,100,1000-290);
//face Lines
line(180,1000-140,170,1000-170);
line(170,1000-170,140,1000-175);
line(220,1000-140,230,1000-170);
line(230,1000-170,260,1000-175);

}
