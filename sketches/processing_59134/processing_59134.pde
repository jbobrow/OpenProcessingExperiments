
size (300, 300);

// Kantenglättung an

smooth();

// color background (r,g,b)

background (200,0,0);

// Außenline

stroke (0,0,255);

point (150,150);

// zeichne eine Linie
// linie (x1,x2,y1,y2)

strokeWeight(10);
line (20,20,280,280);

// keine Außenlinie
// Füllung
// Rechteck schwarz
// Rechteck (x,y,width, height)

noStroke();

fill (250);
rect (50,50,20,20);


// Rechteck transparentes Pink

fill (255,0,200,200);
rect (120,120,30,30);


// Elipse
// (x,y,width, height)
// x und y sind die linke obere Ecke

fill (0);
ellipse (200,200,100,100);




