


void setup() {  //setup function called initially, only once
  size(500, 500);
  background(255);  //set background white
  colorMode(HSB);   //set colors to Hue, Saturation, Brightness mode

}

void draw() {  //draw function loops 
stroke(30,78,214);
fill(137,31,102);
ellipse(400,400,100,100);
rect(200,300,50,50);
  
  
}
