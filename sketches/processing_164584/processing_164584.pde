
//repetition

void setup() {
  size(500, 500);
}

void draw() {
if (key == 'q' || key == 'Q') {
  group();
}

if (key == 'w' || key == 'W') {
  groupTwo();
}

if (key == 'e' || key == 'E') {
  groupThree();
}
}

