//Functions


// parameters takes only two numbers, width and hight
size(500, 500);

// background 0 is black, 255 is white
background(#931818);

// stroke: scale 0-255: 0 is black, 255 is white
stroke(200);

// fill: scale 0-255: 0 is black, 255 is white
fill(100);

//point
point(200, 200);

//line(x1,y1, x2,y2)
line(0,0, 300,300);

//triangle(x1,y1, x2,y2, x3,y3)
triangle(400,300, 400,380, 100,380);

//quad(x1,y1, x2,y2, x3,y3, x4,y4)
quad(100,200, 200,200, 200,300, 100,300);

// fill color RGB 
fill(82, 110, 209);

//ellipse(x,y, width, hight)
ellipse(100,100, 50,80);

// fill color RGB + Alpha(transparent)
fill(82, 110, 209, 40);

//rectangle(x,y, width, hight)
rect(300,100, 50,80);

// nofill 
noFill();
//ellipse(x,y, width, hight)
ellipse(200,200, 50,50);

