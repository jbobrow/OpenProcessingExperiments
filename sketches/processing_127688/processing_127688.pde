


void setup(){
size(500, 500);
background(0,0,255);
}

void draw() {
background(0,0,255);
fill(173,255,41);
ellipse(mouseX,mouseY,150,150);
fill(0);
ellipse(mouseX,mouseY-50,40,40);
fill(255);
ellipse(mouseX,mouseY-50,20,20);
fill(0);
strokeWeight(3);
line(mouseX,mouseY,mouseX+10,mouseY+10);









}

