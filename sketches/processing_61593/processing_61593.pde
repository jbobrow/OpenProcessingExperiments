
void setup () {
  smooth();
  size(500, 500);
  background(67,97,223);
}
void draw() {
  int x;
  x=50;

  fill(7,222,94);
  noStroke();
  ellipse(x,x,x,x);
  ellipse(x+50,x+50,x,x);
  ellipse(x+100,x+100,x,x);
  ellipse(x+150,x+150,x,x);
  ellipse(x+200,x+200,x,x);
  ellipse(x+250,x+250,x,x);
  ellipse(x+300,x+300,x,x);
  ellipse(x+350,x+350,x,x);
  ellipse(x+400,x+400,x,x);}
