void setup(){ 
size (600, 600); 
smooth(); 
background(255);
 } 


  
void draw(){ 
for(int a = 20; a< width; a = a+40){ 
for(int b = 20; b< width; b = b+40){ 

    strokeWeight(15); 
    stroke(255, 13, 74); 
    fill(255); 
    ellipseMode(CENTER); 
    ellipse(a, b,30,30); 
    strokeWeight(20); 


  


  } 


} 


      


      


      


} 

