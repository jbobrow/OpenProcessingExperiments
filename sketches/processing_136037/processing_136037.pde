
size(600,600);
background(255);
int x = 300; 
int y = 300;
int ancho = 400; 
int alto = 300;
smooth();
strokeWeight(2);
fill(255,227,211);
ellipse(x,y,ancho,alto);
fill(0);
ellipse(x-ancho/4,y-alto/5,ancho/20,alto/15);
