
size(500, 500);

background(242, 204, 47);

stroke(255, 0, 0);
strokeWeight(4);
for (int y = 0; y < 100; y += 10) {
  if (y < 100) {
  for (int x = 0; x < 100; x += 10);
   fill ((y+y) * 1.4);
    rect(y, y, 10, 10);
  } else {
    ellipse (35, 35, 167, 167);
  }
}


for (int y = 10; y < 500; y += 20) {
  if (y < 100) {
  for (int x = 50; x < 100; x += 10);
   fill ((y+y) * 1.4);
    rect(y, y, 10, 10);
  } else {
    ellipse (335, 335, 167, 167);
    fill(169, 209, 169);
  }
}







