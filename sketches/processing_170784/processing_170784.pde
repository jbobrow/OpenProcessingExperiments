
float r = 1;


void setup()
{
  size(800, 800);
  background(85,43,230);
  rectMode(CENTER);
  noStroke();
  smooth();
}
  
  void column() {
   for(int y = 0; y < height+25; y = y+50) 
    {
  fill(255);
    rotate(r);
  for (int x = 0; x < width+25; x = x+50)
{  
    
  }
  
  void draw()
  {
    fill(85,43,230,40);
    rect(width / 2,height / 2,width,height);
    fill(255);
    translate(width / 2,height / 2);
    
    column();
    
  
  rect (x,y, 40, 40);
}}
    r = r + 0.01;
    
  } 
  
