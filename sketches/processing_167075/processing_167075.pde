
float[][] eSize = new float[18][5];
float[][] speed = {{0.14, 0.5, 0.3, 0.2, 1.5},
                   {1.0, 2.0, 1.5, 2.0, 0.05},
                   {0.4, 0.8, 0.08, 1.1, 1.4},
                   {0.1, 0.5, 0.3, 0.2, 1.5},    
                   {1.0, 2.0, 1.5, 2.0, 0.05},
                   {0.4, 0.8, 0.08, 1.1, 1.4},
                   {0.1, 0.5, 0.3, 0.2, 1.5},    
                   {1.0, 2.0, 1.5, 2.0, 0.05},
                   {0.4, 0.8, 0.08, 1.1, 1.4},
                   {0.1, 0.5, 0.3, 0.2, 1.5},    
                   {1.0, 2.0, 1.5, 2.0, 0.05},
                   {0.4, 0.8, 0.08, 1.1, 1},
                   {0.1, 0.5, 0.3, 0.2, 1.5},    
                   {1.0, 2.0, 1.5, 2.0, 0.05},
                   {0.4, 0.8, 0.08, 1.1, 2},
                   {0.1, 0.5, 0.3, 0.2, 1.5},    
                   {1.0, 2.0, 1.5, 2.0, 0.05},
                   {0.4, 0.8, 0.08, 1.1, 3}};
float minSize = 1.0;
float maxSize = 160.0;
 
 
void setup() {
  size(150, 150);
  background(255);
  smooth();
  noStroke();
  fill(0);
 
  for(int y = 0; y < 18; y++){  
    for(int x = 0; x < 5; x++){
      eSize[y][x] = x++;
    }
  }
}
 
void draw() {
  background(255);
  for(int y = 6; y < 18; y++){
    for(int x = 0; x < 5; x++){
    for(int z = 4; z < 10; z++){  
      eSize[y][x] += speed[y][x];    
      if(eSize[y][x] > maxSize || eSize[y][x] < minSize) speed[y][x] = -speed[y][x];
      rect(5000*x, 0*y, eSize[y][x], eSize[y][x]);
      scale(0.7, 2.6);
    }
    }
  }
  
}


 
 

 
 
 


